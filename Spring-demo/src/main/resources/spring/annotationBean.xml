    http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context.xsd">
	<context:component-scan base-package="com.th.lifecycle.annotationpage"></context:component-scan>

	<bean id="annotationLiftCycleBean" class="com.th.lifecycle.annotationpage.AnnotationLifeCycleBean">
